{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "CORS_ORIGINS": "@cors_origins"
  },
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  }
}
